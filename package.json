{
    "name": "strife-types",
    "version": "0.0.1",
    "description": "Complete Discord API types for Strife",
    "main": "build/index.js",
    "scripts": {
        "build:dev": "tsc -p tsconfig.dev.json -w",
        "build": "tsc",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "precommit": "pnpm lint && pnpm format"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sussyGaymer/strife-types.git"
    },
    "bugs": {
        "url": "https://github.com/sussyGaymer/strife-types/issues"
    },
    "keywords": [
        "discord",
        "discord-api",
        "discord-types",
        "discord-api-types",
        "discord-selfbot"
    ],
    "files": [
        "build",
        "package.json",
        "README.md"
    ],
    "author": "sussyGaymer",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-formatter-pretty": "^6.0.1",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.5",
        "typescript": "^5.3.3"
    }
